<h1>welcome to homepage</h1>

<div>
    <a class="links" href="/api/destroy-session">Log-Out</a>
</div>

<div>
    <h3>My name: <%= locals.user.username %> </h3>
</div>

<% for (user of all_users){ %>
    <ul>
        <%= user.username%>
        <br>
        <span>Status: </span>
        <%if(user.status == "false"){%>
            <span>Not Available</span>
        <% }else{ %>
            <span>Available</span>
        <% } %>
        
    </ul>
<% } %>

